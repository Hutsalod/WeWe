<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="chat.wewe.android">



    <application
        android:name=".RocketChatApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"

        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppThemeIntro">

        <activity
            android:name="chat.wewe.android.activity.Intro"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />


            </intent-filter>
        </activity>

        <activity
            android:name=".activity.MainActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize">
        <intent-filter>
            <action android:name="android.intent.action.SEND"/>
            <category android:name="android.intent.category.DEFAULT"/>
            <data android:mimeType="image/*"/>
        </intent-filter>/>
        </activity>
        <activity
            android:name=".activity.PrivaryPolicy"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.PinCode"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.PinCodeLong"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.AddServerActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activity.LoginActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize" />

        <activity android:name=".activity.Success"
            android:screenOrientation="portrait"/>
        <activity android:name=".activity.AddTaskActivity"
            android:screenOrientation="portrait"/>

        <activity
            android:name=".activity.room.RoomActivity"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize" />

        <activity
            android:name=".activity.ShowUserDetailed"
            android:configChanges="orientation|screenSize"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize" />


        <activity
            android:name=".ui.MainActivity"
            android:screenOrientation="portrait"
            android:noHistory="true"
            android:turnScreenOn="true"
            android:showWhenLocked="true"/>
        <activity
            android:name=".ui.IncomingActivity"
            android:launchMode="singleInstance"
            android:screenOrientation="portrait"
            android:noHistory="true"
            android:turnScreenOn="true"
            android:showWhenLocked="true"
            />

        <service android:name="chat.wewe.android.service.RocketChatService" />
        <service android:name=".service.PortSipService" />

        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <category android:name="chat.wewe.android" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.google.firebase.iid.FirebaseInstanceIdReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="wewe.rocket.android" />
            </intent-filter>
        </receiver>

        <receiver
            android:name="com.google.firebase.iid.FirebaseInstanceIdInternalReceiver"
            android:exported="false" />

        <service
            android:name="com.google.firebase.iid.FirebaseInstanceIdService"
            android:exported="true">
            <intent-filter android:priority="500">
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>

        <service
            android:name="chat.wewe.android.push.gcm.GCMIntentService"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="wewe.rocket.android" />
            </intent-filter>
        </service>

        <service
            android:name="chat.wewe.android.push.gcm.GcmInstanceIDListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID" />
            </intent-filter>
        </service>

        <receiver
            android:name="chat.wewe.android.push.PushManager$DeleteReceiver"
            android:exported="false" />

        <receiver
            android:name="chat.wewe.android.push.PushManager$ReplyReceiver"
            android:exported="false" />

        <meta-data
            android:name="io.fabric.ApiKey"
            android:value="12ac6e94f850aaffcdff52001af77ca415d06a43" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />

    </application>


    <eat-comment />
    <eat-comment />

    <permission
        android:name="android.permission.RECORD_AUDIO"
        android:description="@string/permdesc_recordAudio"
        android:label="@string/permlab_recordAudio"
        android:permissionGroup="android.permission-group.MICROPHONE"
        android:protectionLevel="dangerous" />
    <permission
        android:name="android.permission.CAMERA"
        android:description="@string/permdesc_camera"
        android:label="@string/permlab_camera"
        android:permissionGroup="android.permission-group.CAMERA"
        android:protectionLevel="dangerous" />

    <permission
        android:name="chat.wewe.android.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />
    <permission-group
        android:name="android.permission-group.MICROPHONE"
        android:description="@string/permgroupdesc_microphone"
        android:icon="@drawable/perm_group_microphone"
        android:label="@string/permgrouplab_microphone"
        android:priority="600" />


    <permission-group
        android:name="android.permission-group.CAMERA"
        android:description="@string/permgroupdesc_camera"
        android:icon="@drawable/perm_group_camera"
        android:label="@string/permgrouplab_camera"
        android:priority="700" />
    <uses-feature
        android:name="android.hardware.camera.autofocus"
        android:required="false" />
    <uses-feature
        android:name="android.hardware.camera.full"
        android:required="false" />
    <uses-feature
        android:name="android.hardware.telephony"
        android:required="false" />

    <uses-permission android:name="chat.wewe.android.permission.C2D_MESSAGE" />
    <uses-permission android:name="android.permission.CALL_LOG" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.FOREGROUND_SERVICE" />
    <uses-permission android:name="com.android.vending.BILLING" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.RAISED_THREAD_PRIORITY" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />
    <uses-permission android:name="android.permission.READ_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RAISED_THREAD_PRIORITY" />

    <uses-feature android:name="android.hardware.camera.autofocus" android:required="false" />
    <uses-feature android:name="android.hardware.camera.full" android:required="false" />
    <uses-feature android:name="android.hardware.telephony" android:required="false" />

    <permission
        android:name="chat.wewe.android.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="chat.wewe.android.permission.C2D_MESSAGE" />

</manifest>